{
    "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
    "name": "SCRAPEO",
    "scopeName": "source.scrapeo",
    "patterns": [
      {
        "name": "comment.line.scrapeo",
        "match": "#.*$"
      },
      {
        "name": "constant.numeric.value.scrapeo",
        "match": "\\b\\d+\\.\\d+\\b"
      },
      {
        "name": "constant.language.boolean.scrapeo",
        "match": "\\b(true|false)\\b"
      },
      {
        "name": "keyword.control.conditional.scrapeo",
        "match": "^\\s*(if|elif|else|endif)\\b"
      },
      {
        "name": "keyword.control.function.begin.scrapeo",
        "match": "^\\s*proc\\b\\s*(\\w+)?"
      },
      {
        "name": "keyword.control.function.end.scrapeo",
        "match": "^\\s*endproc\\b"
      },
      {
        "name": "storage.type.variable.scrapeo",
        "match": "^\\s*var\\b\\s+\\w+"
      },
      {
        "name": "keyword.control.operation.set.scrapeo",
        "match": "^\\s*set\\w*\\b"
      },
      {
        "name": "keyword.control.operation.render.scrapeo",
        "match": "^\\s*render\\w*\\b"
      },
      {
        "name": "keyword.control.operation.call.scrapeo",
        "match": "^\\s*call\\w*\\b"
      },
      {
        "name": "string.unquoted.scrapeo",
        "match": "\\b[a-zA-Z]+\\w*\\b"
      }
    ],
    "repository": {}
}
  