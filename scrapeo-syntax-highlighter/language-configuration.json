{
    "comments": {
      "lineComment": "#"
    },
    "brackets": [
      ["[", "]"],
      ["(", ")"],
      ["{", "}"]
    ],
    "indentationRules": {
      "increaseIndentPattern": "^(\\s*(if|proc)\\b.*)$",
      "decreaseIndentPattern": "^(\\s*(endif|endproc)\\b.*)$"
    },
    "wordPattern": "[A-Za-z0-9_.]+" ,

    "pairs": [
      ["if", "endif"],
      ["proc", "endproc"],
      ["beginevent", "endevent"]
    ]
}
  