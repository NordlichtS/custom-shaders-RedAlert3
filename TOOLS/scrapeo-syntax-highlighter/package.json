{
    "name": "scrapeo-syntax-highlighter",
    "displayName": "SCRAPEO Syntax Highlighter",
    "description": "for RedAlert3 SCRAPEO scripts.",
    "version": "1.2.5",
    "publisher": "NordlichtS",
    "engines": {
      "vscode": "^1.50.0"
    },
    "categories": [
      "Programming Languages"
    ],
    "contributes": {
      "languages": [
        {
          "id": "scrapeo",
          "aliases": ["SCRAPEO", "scrapeo"],
          "extensions": [".SCRAPEO", ".scrape", ".scrapeh"],
          "configuration": "./language-configuration.json"
        }
      ],
      "grammars": [
        {
          "language": "scrapeo",
          "scopeName": "source.scrapeo",
          "path": "./scrapeo.tmLanguage.json"
        }
      ]
    },
    "repository": {
      "type": "git",
      "url": "https://github.com/NordlichtS/custom-shaders-RedAlert3/tree/main/TOOLS/scrapeo-syntax-highlighter"
    },
    "keywords": [
      "syntax highlighting",
      "shader",
      "game development"
    ],
    "_USELESS_icon": "",  
    "scripts": {
      "package": "vsce package"
    },
    "devDependencies": {
      "vsce": "^2.15.0"
    }
  }
  